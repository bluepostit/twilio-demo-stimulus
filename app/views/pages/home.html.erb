<div class="container">
  <h1>Twilio Demo</h1>

  <% if user_signed_in? %>
    <h2>Other users</h2>
    <div class="list-group">
      <% @users.each do |user| %>
        <%= link_to user.name, user_path(user), class: 'list-group-item list-group-item-action' %>
      <% end %>
    </div>
  <% else %>
    <p>You need to sign in to see this content</p>
  <% end %>
</div>
